<div class="modal fade" id="check-search-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header bg-primary text-white">
                <span class="modal-title" id="myModalLabel2">
                    Results for check # "<b>16458*</b>"
                </span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body" style="margin-bottom:-20px">               
                <table class="table">
                    <thead>
                        <tr>
                            <th>Bill Date</th>
                            <th>AP Cycle Name</th>
                            <th>Payment Center ID</th>
                            <th>Payment Center Name</th>
                            <th>Check #</th>
                            <th class="text-right">Total Pharmacy Paid</th>
                            <th class="text-right">Total Claims</th>
                        </tr>
                    </thead>
                    <tbody data-bind="foreach: PayeeDetailRecords">
                        <tr>
                            <td class="text-right" data-bind="text: moment(BillingCycleDate()).format('L')"></td>
                            <td data-bind="text: APCycleName"></td>
                            <td data-bind="text: PaymentCenterId"></td>
                            <td data-bind="text: PaymentCenterName"></td>
                            <td data-bind="text: CheckNumber"></td>
                            <td class="text-right" data-bind="currency: TotalPharmacyPaid"></td>
                            <td class="text-right" data-bind="number: TotalClaims"></td>
                        </tr>
                    </tbody>
                </table>                   
            </div>
            <div class="modal-footer">
                <span class="pull-right">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">
                         <span class="fa fa-external-link-square"></span> Display in Claims Details
                    </button>
                </span>

            </div>
        </div>
    </div>
</div>
