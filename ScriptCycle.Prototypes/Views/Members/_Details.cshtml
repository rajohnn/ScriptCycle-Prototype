<div class="card" style="margin-top:6px">
    <div class="card-header">
        <span class="card-title">Contact Information</span>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-6">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="add1">Address 1</label>
                            <input id="add1" data-bind="textInput: Address.Street1" class="form-control form-control-sm" type="text" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="add2">Address 2</label>
                            <input id="add2" data-bind="textInput: Address.Street2" class="form-control form-control-sm" type="text" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="city">City</label>
                            <input id="city" data-bind="textInput: Address.City" class="form-control form-control-sm" type="text" />
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="form-group">
                            <label for="state">State</label>
                            <select id="state" class="form-control form-control-sm">
                                <option value="AL">AL</option>
                                <option value="AK">AK</option>
                                <option value="AR">AR</option>
                                <option value="AZ">AZ</option>
                                <option value="CA">CA</option>
                                <option value="CO">CO</option>
                                <option value="CT">CT</option>
                                <option value="DC">DC</option>
                                <option value="DE">DE</option>
                                <option value="FL">FL</option>
                                <option value="GA">GA</option>
                                <option value="HI">HI</option>
                                <option value="IA">IA</option>
                                <option value="ID">ID</option>
                                <option value="IL">IL</option>
                                <option value="IN">IN</option>
                                <option value="KS">KS</option>
                                <option value="KY">KY</option>
                                <option value="LA">LA</option>
                                <option value="MA">MA</option>
                                <option value="MD">MD</option>
                                <option value="ME">ME</option>
                                <option value="MI">MI</option>
                                <option value="MN">MN</option>
                                <option value="MO">MO</option>
                                <option value="MS">MS</option>
                                <option value="MT">MT</option>
                                <option value="NC">NC</option>
                                <option value="NE">NE</option>
                                <option value="NH">NH</option>
                                <option value="NJ">NJ</option>
                                <option value="NM">NM</option>
                                <option value="NV">NV</option>
                                <option value="NY">NY</option>
                                <option value="ND">ND</option>
                                <option value="OH">OH</option>
                                <option value="OK">OK</option>
                                <option value="OR">OR</option>
                                <option value="PA">PA</option>
                                <option value="RI">RI</option>
                                <option value="SC">SC</option>
                                <option value="SD">SD</option>
                                <option value="TN">TN</option>
                                <option value="TX">TX</option>
                                <option value="UT">UT</option>
                                <option value="VT">VT</option>
                                <option value="VA">VA</option>
                                <option value="WA">WA</option>
                                <option value="WI">WI</option>
                                <option value="WV">WV</option>
                                <option value="WY">WY</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <label for="zip">Postal Code</label>
                        <input id="zip" data-bind="textInput: Address.Zip" class="form-control form-control-sm" type="text" />
                    </div>
                </div>
            </div>
            <div class="col-lg-6" style="border-left:solid 1px #eee">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="home-phone">Home Phone</label>
                            <input id="home-phone" data-bind="textInput: HomePhone" class="form-control form-control-sm" type="tel" />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="work-phone">Work Phone</label>
                            <input id="work-phone" class="form-control form-control-sm" type="tel" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="mobile-phone">Mobile Phone</label>
                            <input id="mobile-phone" class="form-control form-control-sm" type="tel" />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="fax-phone">Fax</label>
                            <input id="fax-phone" class="form-control form-control-sm" type="tel" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="email-address">Email Address</label>
                            <input id="email-address" data-bind="textInput: EmailAddress" class="form-control form-control-sm" type="email" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row" style="margin-top:6px">
    <div class="col-lg-6">
        <div class="card">
            <div class="card-header">
                <span class="pull-right">
                    <a href="#">
                        <span class="fa fa-plus-circle"></span> Add Diagnosis
                    </a>
                </span>
                <span class="card-title">Diagnosis Codes</span>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="form-group">
                            <label for="d1">Diagnosis Code 1</label>
                            <input id="d1" class="form-control form-control-sm" value="Z32.0" />
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="form-group">
                            <label for="d1">Diagnosis Code 2</label>
                            <input id="d1" class="form-control form-control-sm" value="Z32.0" />
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="form-group">
                            <label for="d1">Diagnosis Code 3</label>
                            <input id="d1" class="form-control form-control-sm" value="Z32.0" />
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="card">
            <div class="card-header">
                <span class="card-title">Additional Information</span>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="relation-code">Relationship Code</label>
                            <input id="relation-code" class="form-control form-control-sm" type="text" />
                        </div>
                    </div>
                    <div class="col-lg-6">

                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
<div class="row" style="margin-top:6px">
    <div class="col-lg-6">
        <div class="card">
            <div class="card-header">
                <span class="card-title">Primacy Care Physician</span>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="pcp-npi">PCP NPI</label>
                            <div class="input-group">
                                <input id="pcp-npi" class="form-control form-control-sm" placeholder="Search..." />
                                <div class="input-group-append">
                                    <button type="button" class="btn btn-sm btn-primary">
                                        <span class="fa fa-search"></span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="pcp-phone">PCP Phone</label>
                            <input id="pcp-phone" class="form-control form-control-sm" />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="pcp-fax">PCP Fax</label>
                            <input id="pcp-fax" class="form-control form-control-sm" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="card">
            <div class="card-header">
                <span class="card-title">Pharmacy Information</span>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="pharm-ncpdp">Pharmacy NCPDP</label>
                            <div class="input-group">
                                <input id="pharm-ncpdp" class="form-control form-control-sm" placeholder="Search..." />
                                <div class="input-group-append">
                                    <button type="button" class="btn btn-sm btn-primary">
                                        <span class="fa fa-search"></span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="pharm-phone">Pharmacy Phone</label>
                            <input id="pharm-phone" class="form-control form-control-sm" />
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="pharm-fax">Pharmacy Fax</label>
                            <input id="pharm-fax" class="form-control form-control-sm" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
