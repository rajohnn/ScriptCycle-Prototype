<div class="modal right fade" id="create-dependent-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">

        <div class="modal-content">
            <div class="modal-header">
                <span class="modal-title" id="myModalLabel2">
                    Create Dependent for <span data-bind="text: MemberModel.FirstName"></span> <span data-bind="text:MemberModel.LastName"></span>
                </span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>

            </div>
            
            <div class="modal-body" style="margin:0!important">
                <table class="table bg-info text-white" style="width:500px;margin-top:-15px;margin-left:-15px">
                    <tr>
                        <td align="center">
                            Group Number: 2342342
                        </td>
                        <td align="center">
                            Effective Date: 06/01/2018
                        </td>
                        <td align="center">
                            Term Date: 05/31/2019
                        </td>
                    </tr>
                </table>  
                <section id="dependent-data-entry" class="text-primary">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-member-id">CH ID</label>
                                <div id="new-member-id" class="form-control form-control-sm" type="text">23424234</div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-person-code">Person Code</label>
                                <input id="new-person-code" class="form-control form-control-sm" type="text" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-first-name">First Name</label>
                                <input id="new-first-name" class="form-control form-control-sm" type="text" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-lastname">Last Name</label>
                                <input id="new-lastname" class="form-control form-control-sm" type="text" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-dob">Date of Birth</label>
                                <input id="new-dob" class="form-control form-control-sm" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-ssn">SSN</label>
                                <input id="new-ssn" class="form-control form-control-sm" type="text" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-gender">Gender</label>
                                <select id="new-gender" class="form-control form-control-sm">
                                    <option>Male</option>
                                    <option>Female</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-relationship">Relationship Code</label>
                                <input id="new-relationship" class="form-control form-control-sm" type="text" />
                            </div>
                        </div>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-effective-date">Effective Date</label>
                                <input id="new-effective-date" class="form-control form-control-sm" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="new-term-date">Termination Date</label>
                                <input id="new-term-date" class="form-control form-control-sm" />
                            </div>
                        </div>
                    </div>
                    <hr />
                </section>
               
            </div>
           
            <div class="modal-footer">
                <span class="pull-right">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Create Dependent</button>
                </span>
                
            </div>
        </div>

    </div>
</div>

