<div class="card" style="margin-top:6px">
    <div class="card-header">
        <span class="card-title">Main Member Data</span>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="first-name">First Name</label>
                    <input id="first-name" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="middle-name">Middle Name</label>
                    <input id="middle-name" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="last-name">Last Name</label>
                    <input id="last-name" class="form-control form-control-sm" type="text" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="SSN">SSN</label>
                    <input id="SSN" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="Gender">Gender</label>
                    <select id="gender-options" class="form-control form-control-sm ">
                        <option>Male</option>
                        <option selected>Female</option>
                    </select>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="date-of-birth">Date of Birth</label>
                    <input id="date-of-birth" class="form-control form-control-sm" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="carrier-code">Carrier Code</label>
                    <input id="carrier-code" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="carrier-name">Carrier Name</label>
                    <input id="carrier-name" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="enrollment-method">Enrollment Method</label>
                    <select id="enrollment-method" class="form-control form-control-sm">
                        <option>ADI</option>
                        <option>MES</option>
                        <option>MLS</option>
                        <option>WEB</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="case-manager">Case Manager</label>
                    <input id="case-manager" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="alt-member-id">Alternative Member ID</label>
                    <input id="alt-member-id" class="form-control form-control-sm" type="text" />
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label for="prev-member-id">Previous Member ID</label>
                    <input id="prev-member-id" class="form-control form-control-sm" type="text" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4">
                <label for="language-pref">Preferred Language</label>
                <select id="language-pref" class="form-control form-control-sm ">
                    <option>Arabic</option>
                    <option>Chinese</option>
                    <option selected>English</option>
                    <option>Farsi</option>
                    <option>German</option>
                    <option>Greek</option>
                    <option>Haitian Creole</option>
                    <option>Italian</option>
                    <option>Korean</option>
                    <option>Navajo</option>
                    <option>Polish</option>
                    <option>Portuguese</option>
                    <option>Russian</option>
                    <option>Spanish</option>
                    <option>Tagalog</option>
                    <option>Vietnamese</option>
                </select>
            </div>
            <div class="col-lg-4">
                <label for="created-date">Date Created</label>
                <div class=" form-control form-control-sm">Oct. 16, 2018 10:04am</div>
            </div>
            <div class="col-lg-4">
                <label for="card-print-date">Card Print Date</label>

                <div class=" form-control form-control-sm"><span class="fa fa-print"></span> Oct. 1, 2018 11:30pm</div>

            </div>
        </div>
    </div>
</div>
<div class="card" style="margin-top:7px;margin-bottom:7px">
    <div class="card-header">
        <span class="card-title">Member Adjustment Options</span>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-4">
                <label><input type="checkbox" /> Reject Primary Claims</label>
            </div>
            <div class="col-lg-4">
                <label><input type="checkbox" /> Ignore Updates from Imports</label>
            </div>
        </div>
    </div>
</div>
